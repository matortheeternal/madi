<div class="scaffold">
  <p id="notice"><%= notice %></p>

  <h1>merge_reports</h1>

  <table>
    <thead>
      <tr>
          <th>user_id</th>
          <th>approved</th>
          <th>game</th>
          <th>filename</th>
          <th>hash</th>
          <th>record_count</th>
          <th>rating</th>
          <th>merge_version</th>
          <th>notes</th>
          <th>approved_at</th>
          <th colspan="3"></th>
      </tr>
    </thead>

    <tbody>
      <% @merge_reports.each do |merge_report| %>
        <tr>
            <td><%= merge_report.user_id %></td>
            <td><%= merge_report.approved %></td>
            <td><%= merge_report.game %></td>
            <td><%= merge_report.filename %></td>
            <td><%= merge_report.hash %></td>
            <td><%= merge_report.record_count %></td>
            <td><%= merge_report.rating %></td>
            <td><%= merge_report.merge_version %></td>
            <td><%= merge_report.notes %></td>
            <td><%= merge_report.approved_at %></td>
            <td><%= link_to 'Show', merge_report %></td>
          <td><%= link_to 'Edit', edit_merge_report_path(merge_report) %></td>
          <td><%= link_to 'Destroy', merge_report, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        </tr>
      <% end %>
    </tbody>
  </table>

  <br>

  <%= link_to 'New Merge Report', new_merge_report_path %>
</div>